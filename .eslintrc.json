{
    "parserOptions": {
        "ecmaVersion": 8,
        "sourceType": "module",
        "env": {
            "node": true
        },
        "indent": ["error", "tab"]
    },
    "plugins": ["async-await"],
    "rules": {
        "async-await/space-after-async": 2,
        "async-await/space-after-await": 2
    }
}